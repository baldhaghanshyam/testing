<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation=" http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.1.xsd">

	<context:annotation-config />

	<bean class="com.springcore.auto.wired.annotation.Address"
		name="address">
		<property name="street" value="varachha" />
		<property name="city" value="srat" />
	</bean>
	
	<bean class="com.springcore.auto.wired.annotation.Address"
		name="temp2">
		<property name="street" value="filmcity road" />
		<property name="city" value="mumbai" />
	</bean>
	<bean class="com.springcore.auto.wired.annotation.Emp" name="emp1">

	</bean>
</beans>